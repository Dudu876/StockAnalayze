<Window x:Class="StockAnalayze.src.StatusWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockAnalayze.src"
        mc:Ignorable="d"
        Title="Analayzing..." Height="171.711" Width="477.632" ResizeMode="NoResize">
    <Grid>
        <ProgressBar HorizontalAlignment="Left" Height="17" Margin="76,82,0,0" VerticalAlignment="Top" Width="309" Value="{Binding Path=Progress, Mode=TwoWay}"/>
        <Label x:Name="label" Content="{Binding Path=Status, Mode=TwoWay}" HorizontalAlignment="Left" Margin="76,34,0,0" VerticalAlignment="Top" Height="30" Width="309"/>

    </Grid>
</Window>
